    "orm",
    "waterline",
    "sails"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "async": "~0.9.0",
    "lodash": "~3.8.0",
    "mysql": "~2.6.1",
    "waterline-errors": "~0.10.0",
